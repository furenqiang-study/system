<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.furenqiang.system.mapper.SysChunkFilesMapper">
    <resultMap id="BaseResultMap" type="com.furenqiang.system.entity.SysChunkFiles">
        <result column="id" property="id" jdbcType="INTEGER" />
        <result column="target_id" property="targetId" jdbcType="VARCHAR" />
        <result column="file_path" property="filePath" jdbcType="VARCHAR" />
        <result column="file_name" property="fileName" jdbcType="VARCHAR" />
        <result column="suffix" property="suffix" jdbcType="VARCHAR" />
        <result column="create_time" property="createTime" jdbcType="VARCHAR" />
        <result column="modified_time" property="modifiedTime" jdbcType="VARCHAR" />
        <result column="deleted" property="deleted" jdbcType="INTEGER" />
    </resultMap>

    <sql id="Base_Column_List">
		id,target_id,file_path,file_name,suffix,create_time,modified_time,deleted
	</sql>

    <select id="fileIsExist" resultType="java.lang.Boolean">
        select count(id) > 0
        from sys_chunk_files
        where deleted = 0 and file_path = #{fileName}
    </select>
</mapper>